        while cnt > 0 :
            if 's' in str(key) :
                if orbital[key] < 2 :
                    cnt = cnt - 1
                    orbital[key] = orbital[key] + a.pop()
                    continue
                else :
                    break
            elif 'p' in str(key) :
                if orbital[key] < 6 :
                    cnt = cnt - 1
                    orbital[key] = orbital[key] + a.pop()
                    continue
                else :
                    break
            elif 'd' in str(key) :
                if orbital[key] < 10 :
                    cnt = cnt - 1
                    orbital[key] = orbital[key] + a.pop()
                    continue
                else :
                    break
            elif 'f' in str(key) :
                if orbital[key] < 14 :
                    cnt = cnt - 1
                    orbital[key] = orbital[key] + a.pop()
                    continue
                else :
                    break
